<h2>Show Session</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @session.name %>
  </li>

  <li>
    <strong>Lastname:</strong>
    <%= @session.lastname %>
  </li>

</ul>

<span><%= link "Edit", to: session_path(@conn, :edit, @session) %></span>
<span><%= link "Back", to: session_path(@conn, :index) %></span>
